{% if title and subtitle %}
  {% set completetitle = title + ": " + subtitle + " - " + metadata.name %}
{% elif title %}
  {% set completetitle = title + " - " + metadata.name %}
{% else %}
  {% set completetitle = metadata.name %}
{% endif %}
<!doctype html>
<html lang="{{ language }}">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ completetitle }}</title>
    <link rel="canonical" href="{{ metadata.rootURI }}{{ page.url }}">
    <link rel="icon" href="{{ metadata.icons.ico.href }}" sizes="{{ metadata.icons.ico.sizes }}"/>
    <link rel="icon" href="{{ metadata.icons.svg.href }}" type="image/svg+xml"/>
    <link rel="apple-touch-icon" href="{{ metadata.icons.apple.href }}"/>
    
    <link rel="stylesheet" media="all" href="/{{ styles['src/assets/styles/reset.css'] }}">
    <link rel="stylesheet" media="all" href="/{{ styles['src/assets/styles/theme.css'] }}">
    <link rel="stylesheet" media="all" href="/{{ styles['src/assets/styles/cv.css'] }}">
    <link rel="stylesheet" media="print" href="/{{ styles['src/assets/styles/cv-print.css'] }}">
    {% block styles %}{% endblock %}

    <meta name="referrer" content="origin"/>
    <meta name="robots" content="index, follow"/>
    <meta name="generator" content="{{ elevPenty.generator }}">
    {% if metadata.theme.color %}
      <meta name="theme-color" content="{{ metadata.theme.color }}">
    {% endif %}
    {% if metadata.googleOwnership %}
      {% for ownership in metadata.googleOwnership %}
        <meta name="google-site-verification" content="{{ ownership }}"/>
      {% endfor %}
    {% endif %}
  
    {% block meta %}
      <meta name="description" content="{{ heading }}"/>
      <meta name="author" content="{{ metadata.author.name }}"/>
      <meta name="generator" content="11ty 0.10.0"/>
      <meta property="og:type" content="website"/>
      <meta property="og:image" content="{{ metadata.rootURI }}{{ metadata.images.og }}"/>
      <meta property="og:image:alt" content="{{ metadata.description }}"/>
      <meta property="og:site_name" content="{{ metadata.name }}"/>
      <meta property="og:title" content="{{ completetitle }}"/>
      <meta property="og:description" content="{{ heading }}"/>
      <meta property="og:url" content="{{ metadata.rootURI }}{{ page.url }}"/>
      <meta property="og:locale" content="{{ language }}"/>
      <meta name="twitter:card" content="summary_large_image"/>
      {% if metadata.twitter %}
      <meta name="twitter:creator" content="@{{ metadata.twitter }}"/>
      {% endif %}
      <meta name="twitter:image:src" content="{{ metadata.rootURI }}{{ metadata.images.image }}"/>
      <meta name="twitter:title" content="{{ completetitle }}"/>
      <meta name="twitter:description" content="{{ heading }}"/>
      <meta name="twitter:url" content="{{ metadata.rootURI }}{{ page.url }}"/>
    {% endblock %}

    <script defer src="/{{ scripts['src/assets/scripts/cv.js'] }}"></script>
    {% block scripts %}{% endblock %}
  </head>
  <body>
